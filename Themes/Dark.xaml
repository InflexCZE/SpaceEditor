<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Background" Color="#151515"/>
    <SolidColorBrush x:Key="Foreground" Color="White"/>

    <SolidColorBrush x:Key="DisabledBackgroundBrush" Color="Black" />
    <SolidColorBrush x:Key="DisabledForegroundBrush" Color="Gray" />
    <SolidColorBrush x:Key="SelectedBackgroundBrush" Color="#404040" />
    <SolidColorBrush x:Key="MouseoverBackgroundBrush" Color="#202020" />
    <SolidColorBrush x:Key="UnselectedBackgroundBrush" Color="#050505" />
    <SolidColorBrush x:Key="Border" Color="#AAA" />
</ResourceDictionary>